<?xml version="1.0" encoding="utf-8"?>
<layer-list  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <solid android:color="#e9e6e1" />
            <corners android:topLeftRadius="0.1dp" android:topRightRadius="0.1dp" android:bottomLeftRadius="0.1dp" android:bottomRightRadius="0.1dp" />
            <stroke android:width="1dp" android:color="#ffd8d8d8" />
        </shape>
    </item>
    <item android:left="1dp" android:top="1dp" android:right="1dp" android:bottom="1dp">
        <shape>
            <solid android:color="#e9e6e1" />
            <corners android:topLeftRadius="0.1dp" android:topRightRadius="0.1dp" android:bottomLeftRadius="0.1dp" android:bottomRightRadius="0.1dp" />
            <stroke android:width="1dp" android:color="#ffffffff" />
        </shape>
    </item>
</layer-list>
